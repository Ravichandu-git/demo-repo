pipeline{
		agent{ 
			label'maven-2'
			}
			stages{
				stage('checkout-1') {
				steps {
				git branch: 'main', url: 'https://github.com/Ravichandu-git/java-example.git'
				}
			}
				stage('test-1') {
				steps {
				echo 'testing my own jenkins'
				}			
			}
		
				stage('build-1') {
				steps {
				sh 'mvn clean install'
				}			
				}		
				stage('deploy-1') {
				steps {
				sh sudo cp /home/ubuntu/rc/worlspace/testQ/target/*.war /opt/tomcat/
			     }			
		    }		
		
		
	}

}
